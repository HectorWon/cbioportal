# app-name (used by at least authentication)
app.name=
app.version=

# database
db.tomcat_resource_name=jdbc/cbioportal

# web page cosmetics
skin.title=cBio GDAC Portal
skin.email_contact=cbioportal at googlegroups dot com
skin.blurb=<b>Data in this portal is based on the 26 October 2011 Broad Firehose run of TCGA data.  Additional mutation files have been added from each disease working group.</b>
skin.authorization_message=Access to this portal is only available to authorized users who are involved in TCGA-funded work or are active participants of the TCGA Disease Working Groups or Analysis Working Groups.  [<a href="http://bit.ly/gdac-form">Request Access</a>].
skin.right_nav.show_testimonials=true
skin.data_sets_header=The portal currently contains data from the following TCGA cancer genomics studies.  The table below lists the number of available samples per data type and tumor.
skin.data_sets_footer=Based on the Firehose run from 12/30/2011.
faq=faq_tcga.html
about=about_us.html
news=news_tcga.html
examples_right_column=examples_tcga.html

# pathway settings
include_networks=true
pathway_commons.url=http://www.pathwaycommons.org/pc2

# authentication
authenticate=googleplus
authorization=true
googleplus.consumer.key=
googleplus.consumer.secret=

# bitly
bitly.user=
bitly.api_key=

# patient view settings
patient_view_placeholder=false
patient_view_genomic_overview_cna_cutoff=0.2,1.5
digitalslidearchive.iframe.url=http://cancer.digitalslidearchive.net/index_mskcc.php?slide_name=
digitalslidearchive.meta.url=http://cancer.digitalslidearchive.net/local_php/get_slide_list_from_db_groupid_not_needed.php?slide_name_filter=
tumor_image.url=http://cbio.mskcc.org/cancergenomics/tcga-tumor-images/
tcga_path_report.url=https://tcga-data.nci.nih.gov/tcgafiles/ftp_auth/distro_ftpusers/anonymous/tumor/{cancer.type}/bcr/nationwidechildrens.org/pathology_reports/reports/

# igv bam linking
igv.bam.linking=
igv.bam.linking.studies=
openssl.binary=
signature.key=
encryption.key=
broad.bam.url=
broad.bam.checking.url=

# genomespace linking
genomespace=true

# urls
